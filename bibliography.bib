@online{attieModelProgramRepair2007,
  title = {Model and {{Program Repair}} via {{SAT Solving}}},
  author = {Attie, Paul C. and Saklawi, Jad},
  date = {2007-10-17},
  eprint = {0710.3332},
  eprinttype = {arXiv},
  url = {http://arxiv.org/abs/0710.3332},
  urldate = {2018-06-05},
  abstract = {We consider the following \textbackslash textbackslash emph\{model repair problem\}: given a finite Kripke structure \$M\$ and a specification formula \$\textbackslash textbackslash eta\$ in some modal or temporal logic, determine if \$M\$ contains a substructure \$M'\$ (with the same initial state) that satisfies \$\textbackslash textbackslash eta\$. Thus, \$M\$ can be “repaired” to satisfy the specification \$\textbackslash textbackslash eta\$ by deleting some transitions. We map an instance \$(M, \textbackslash textbackslash eta)\$ of model repair to a boolean formula \$\textbackslash textbackslash repfor(M,\textbackslash textbackslash eta)\$ such that \$(M, \textbackslash textbackslash eta)\$ has a solution iff \$\textbackslash textbackslash repfor(M,\textbackslash textbackslash eta)\$ is satisfiable. Furthermore, a satisfying assignment determines which transitions must be removed from \$M\$ to generate a model \$M'\$ of \$\textbackslash textbackslash eta\$. Thus, we can use any SAT solver to repair Kripke structures. Furthermore, using a complete SAT solver yields a complete algorithm: it always finds a repair if one exists. We extend our method to repair finite-state shared memory concurrent programs, to solve the discrete event supervisory control problem \textbackslash textbackslash cite\{RW87,RW89\}, to check for the existence of symmettric solutions \textbackslash textbackslash cite\{ES93\}, and to accomodate any boolean constraint on the existence of states and transitions in the repaired model. Finally, we show that model repair is NP-complete for CTL, and logics with polynomial model checking algorithms to which CTL can be reduced in polynomial time. A notable example of such a logic is Alternating-Time Temporal Logic (ATL).},
  keywords = {Computer Science - Logic in Computer Science,D.2.2,F.3.1,F.4.1}
}

