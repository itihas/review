@article{suEvolutionaryDynamicsGame2019,
  title = {Evolutionary Dynamics with Game Transitions},
  author = {Su, Qi and McAvoy, Alex and Wang, Long and Nowak, Martin A.},
  date = {2019-12-17},
  journaltitle = {Proceedings of the National Academy of Sciences},
  volume = {116},
  number = {51},
  pages = {25398--25404},
  publisher = {Proceedings of the National Academy of Sciences},
  doi = {10.1073/pnas.1908936116},
  url = {https://www.pnas.org/doi/full/10.1073/pnas.1908936116},
  urldate = {2023-05-26},
  abstract = {The environment has a strong influence on a population’s evolutionary dynamics. Driven by both intrinsic and external factors, the environment is subject to continual change in nature. To capture an ever-changing environment, we consider a model of evolutionary dynamics with game transitions, where individuals’ behaviors together with the games that they play in one time step influence the games to be played in the next time step. Within this model, we study the evolution of cooperation in structured populations and find a simple rule: Weak selection favors cooperation over defection if the ratio of the benefit provided by an altruistic behavior,  b b , to the corresponding cost,  c c , exceeds  k−k' k−k′ , where  k k  is the average number of neighbors of an individual and  k' k′  captures the effects of the game transitions. Even if cooperation cannot be favored in each individual game, allowing for a transition to a relatively valuable game after mutual cooperation and to a less valuable game after defection can result in a favorable outcome for cooperation. In particular, small variations in different games being played can promote cooperation markedly. Our results suggest that simple game transitions can serve as a mechanism for supporting prosocial behaviors in highly connected populations.},
  file = {/home/itihas/Zotero/storage/5AEDBE8G/Su et al. - 2019 - Evolutionary dynamics with game transitions.pdf;/home/itihas/Zotero/storage/S6FJLEZA/pnas.1908936116.pdf.pdf}
}

